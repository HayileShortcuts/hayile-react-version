{
  "extends": "astro/tsconfigs/strict",
  "include": [
    ".astro/types.d.ts",
    "**/*"
  ],
  "exclude": [
    "dist"
  ],
  "compilerOptions": {
    "strict": false,
    "baseUrl": ".",
    "paths": {
      "@/*": [
        "src/*"
      ],
      "components/*": [
        "components/*"
      ],
      "assets/*": [
        "assets/*"
      ],
      "types/*": [
        "types/*"
      ]
    },
    "jsx": "react-jsx",
    "jsxImportSource": "preact"
  }
}